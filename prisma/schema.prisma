generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserMap {
  id        String   @id  @default(uuid())
  
  userId    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  title     String

  mapUrl    String
  previewMapUrl    String

  mapData   Json?
  // mapData : {
  //  icons : Json?
  //  texts : Json?
  // }
  notes   String?
}

model Map {
  id        String   @id  @default(uuid())
  title     String

  mapUrl    String
  previewMapUrl    String
}
