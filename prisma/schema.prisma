generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserMap {
  id        String   @id  @default(uuid())
  
  userId    String
  createdAt DateTime @default(now())
  title     String

  mapUrl    String
  previewMapUrl    String

  icons     Json?
}

model Map {
  id        String   @id  @default(uuid())
  title     String

  mapUrl    String
  previewMapUrl    String
}